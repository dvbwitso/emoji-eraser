{
  "name": "emoji-eraser",
  "displayName": "Emoji Eraser â€” Clean AI-generated code artifacts instantly",
  "description": "Clean AI-generated code: remove emojis, debug statements, AI comments, and trailing whitespace. Perfect for code generated by GitHub Copilot, ChatGPT, and other AI tools.",
  "version": "1.1.1",
  "publisher": "DabwitsoMweemba",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Formatters",
    "Linters",
    "Other"
  ],
  "keywords": [
    "emoji",
    "remove",
    "clean",
    "formatter",
    "lint",
    "productivity",
    "ai",
    "code-quality",
    "unicode",
    "debug",
    "copilot",
    "ai-cleanup",
    "whitespace",
    "code-formatter"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "emoji-eraser.removeFromFile",
        "title": "Remove Emojis from Current File",
        "category": "Emoji Eraser"
      },
      {
        "command": "emoji-eraser.removeFromWorkspace",
        "title": "Remove Emojis from Workspace",
        "category": "Emoji Eraser"
      },
      {
        "command": "emoji-eraser.removeFromComments",
        "title": "Remove Emojis from Comments Only",
        "category": "Emoji Eraser"
      },
      {
        "command": "emoji-eraser.removeFromStrings",
        "title": "Remove Emojis from Strings Only",
        "category": "Emoji Eraser"
      },
      {
        "command": "emoji-eraser.removeTrailingWhitespace",
        "title": "Remove Trailing Whitespace",
        "category": "Emoji Eraser"
      },
      {
        "command": "emoji-eraser.removeDebugStatements",
        "title": "Remove Debug Statements",
        "category": "Emoji Eraser"
      },
      {
        "command": "emoji-eraser.removeAIComments",
        "title": "Remove AI Comment Markers",
        "category": "Emoji Eraser"
      },
      {
        "command": "emoji-eraser.cleanAllArtifacts",
        "title": "Clean All AI Artifacts",
        "category": "Emoji Eraser"
      }
    ],
    "configuration": {
      "title": "Emoji Eraser",
      "properties": {
        "emojiEraser.fileTypes": {
          "type": "array",
          "default": [
            "javascript",
            "typescript",
            "python",
            "java",
            "markdown",
            "json",
            "html",
            "css"
          ],
          "description": "File types to scan for emojis"
        },
        "emojiEraser.showNotifications": {
          "type": "boolean",
          "default": true,
          "description": "Show notifications with emoji removal summary"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "echo \"No tests configured yet\" && exit 0",
    "lint": "echo \"No linter configured yet\" && exit 0",
    "package": "vsce package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dvbwitso/emoji-eraser"
  },
  "author": "Dabwitso Mweemba",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dvbwitso/emoji-eraser/issues"
  },
  "homepage": "https://github.com/dvbwitso/emoji-eraser#readme",
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/vscode": "^1.105.0",
    "@vscode/vsce": "^3.2.1",
    "typescript": "^5.9.3"
  }
}
